<div class="row">
	<div class="col-lg-12">

		<%= form_for(@modules_category) do |f| %>

			<div class="form-group">
				<label><%= f.label :catname, "Category Name" %></label>
				<%= f.text_field :catname, class: "form-control" %>
			</div>

			<div class="actions">
				<%= f.submit "Submit", class: "btn btn-primary" %>
			</div>

		<% end %>

	</div>
</div>
